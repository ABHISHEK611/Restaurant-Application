<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_contact_restoran"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.abhishek.restoran.Contact_restoran"
    android:background="@drawable/foodbackground">

    <ImageView
        android:id="@+id/iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Contact-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="167dp"
        android:id="@+id/tv"
        android:textSize="24sp"
        android:textStyle="normal|bold|italic"
        android:fontFamily="casual"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:text="Tollfree No.-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="58dp"
        android:id="@+id/tv1"
        android:layout_alignTop="@+id/tv"
        android:layout_alignRight="@+id/tv"
        android:layout_alignEnd="@+id/tv"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp"
        android:fontFamily="casual"
        android:textStyle="normal|bold" />

    <TextView
        android:text="Mobile No.-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv3"
        android:fontFamily="casual"
        android:textStyle="normal|bold"
        android:layout_below="@+id/tv1"
        android:layout_alignLeft="@+id/tv1"
        android:layout_alignStart="@+id/tv1" />

    <TextView
        android:text="Timing-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv3"
        android:layout_alignLeft="@+id/tv3"
        android:layout_alignStart="@+id/tv3"
        android:id="@+id/tv5"
        android:fontFamily="casual"
        android:textStyle="normal|bold" />

    <TextView
        android:text="Email id-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv6"
        android:layout_alignLeft="@+id/tv5"
        android:layout_alignStart="@+id/tv5"
        android:id="@+id/tv7"
        android:fontFamily="casual"
        android:textStyle="normal|bold"
        android:textSize="14sp" />

    <TextView
        android:text="2017"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv9"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/tv7"
        android:layout_alignStart="@+id/tv7" />

    <TextView
        android:id="@+id/tv10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:text="Copr.AbhiSoft Pvt Ltd" />

    <TextView
        android:text="restoran.17@gmail.com"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv8"
        android:fontFamily="casual"
        android:textStyle="normal|bold"
        android:layout_below="@+id/tv6"
        android:layout_alignLeft="@+id/tv6"
        android:layout_alignStart="@+id/tv6" />

    <TextView
        android:text="1800-3377-3344"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv2"
        android:fontFamily="casual"
        android:textStyle="normal|bold"
        android:layout_above="@+id/tv4"
        android:layout_toRightOf="@+id/tv1"
        android:layout_toEndOf="@+id/tv1" />

    <TextView
        android:text="8090947355"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv4"
        android:fontFamily="casual"
        android:textStyle="normal|bold"
        android:layout_above="@+id/tv6"
        android:layout_toRightOf="@+id/tv1"
        android:layout_toEndOf="@+id/tv1" />

    <TextView
        android:text="Monday-Saturday (8am-11pm)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv6"
        android:fontFamily="casual"
        android:textStyle="normal|bold"
        android:layout_below="@+id/tv3"
        android:layout_alignLeft="@+id/tv4"
        android:layout_alignStart="@+id/tv4" />

</RelativeLayout>
